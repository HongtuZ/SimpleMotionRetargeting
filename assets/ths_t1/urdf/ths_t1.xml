    <!-- 传感器配置 -->
    <sensor>

        <!--      位置传感器名称  关联的关节名称                   无测量噪声 --> 
        <!-- 左腿 -->
        <jointpos name="llpjp" joint="left_leg_pitch_joint"  noise="0.0"/> 
        <jointpos name="llrjp" joint="left_leg_roll_joint"   noise="0.0"/>
        <jointpos name="llyjp" joint="left_leg_yaw_joint"    noise="0.0"/>
        <jointpos name="llkjp" joint="left_leg_knee_joint"   noise="0.0"/>
        <jointpos name="llajp" joint="left_leg_ankle_joint"  noise="0.0"/>
        <!-- 右腿 -->
        <jointpos name="rlpjp" joint="right_leg_pitch_joint"  noise="0.0"/> 
        <jointpos name="rlrjp" joint="right_leg_roll_joint"   noise="0.0"/>
        <jointpos name="rlyjp" joint="right_leg_yaw_joint"    noise="0.0"/>
        <jointpos name="rlkjp" joint="right_leg_knee_joint"   noise="0.0"/>
        <jointpos name="rlajp" joint="right_leg_ankle_joint"  noise="0.0"/>

        <!-- 腰部 -->
        <jointpos name="bm1jp" joint="body_m1_joint"        noise="0.0"/>

        <!-- 左手臂 -->
        <jointpos name="la1jp" joint="left_arm_m1_joint"       noise="0.0"/>
        <jointpos name="la2jp" joint="left_arm_m2_joint"       noise="0.0"/>
        <jointpos name="la3jp" joint="left_arm_m3_joint"       noise="0.0"/>
        <jointpos name="la4jp" joint="left_arm_m4_joint"       noise="0.0"/>
        <jointpos name="la5jp" joint="left_arm_m5_joint"       noise="0.0"/>
        <!-- <jointpos name="la6jp" joint="left_arm_m6_joint"       noise="0.0"/>   -->
        <!-- 右手臂 --> 
        <jointpos name="ra1jp" joint="right_arm_m1_joint"      noise="0.0"/>
        <jointpos name="ra2jp" joint="right_arm_m2_joint"      noise="0.0"/>
        <jointpos name="ra3jp" joint="right_arm_m3_joint"      noise="0.0"/>
        <jointpos name="ra4jp" joint="right_arm_m4_joint"      noise="0.0"/>
        <jointpos name="ra5jp" joint="right_arm_m5_joint"      noise="0.0"/>
        <!-- <jointpos name="ra6jp" joint="right_arm_m6_joint"      noise="0.0"/> -->
        
  

        <!--      速度传感器名称  关联的关节名称                   无测量噪声 -->
        <!-- 左腿 -->
        <jointvel name="llpjv" joint="left_leg_pitch_joint"   noise="0.0"/> 
        <jointvel name="llrjv" joint="left_leg_roll_joint"    noise="0.0"/>
        <jointvel name="llyjv" joint="left_leg_yaw_joint"     noise="0.0"/>
        <jointvel name="llkjv"  joint="left_leg_knee_joint"   noise="0.0"/>
        <jointvel name="llajv"  joint="left_leg_ankle_joint"  noise="0.0"/>
        <!-- 右腿 -->
        <jointvel name="rlpjv" joint="right_leg_pitch_joint"  noise="0.0"/> 
        <jointvel name="rlrjv" joint="right_leg_roll_joint"   noise="0.0"/>
        <jointvel name="rlyjv" joint="right_leg_yaw_joint"    noise="0.0"/>
        <jointvel name="rlkjv" joint="right_leg_knee_joint"  noise="0.0"/>
        <jointvel name="rlajv" joint="right_leg_ankle_joint" noise="0.0"/>

        <!-- 腰部 -->
        <jointvel name="bm1jv" joint="body_m1_joint"           noise="0.0"/>

        <!-- 左手臂 -->
        <jointvel name="la1jv" joint="left_arm_m1_joint"       noise="0.0"/>
        <jointvel name="la2jv" joint="left_arm_m2_joint"       noise="0.0"/>
        <jointvel name="la3jv" joint="left_arm_m3_joint"       noise="0.0"/>
        <jointvel name="la4jv" joint="left_arm_m4_joint"       noise="0.0"/>
        <jointvel name="la5jv" joint="left_arm_m5_joint"       noise="0.0"/>
        <!-- <jointvel name="la6jv" joint="left_arm_m6_joint"       noise="0.0"/> -->
        <!-- 右手臂 -->
        <jointvel name="ra1jv" joint="right_arm_m1_joint"      noise="0.0"/>
        <jointvel name="ra2jv" joint="right_arm_m2_joint"      noise="0.0"/>
        <jointvel name="ra3jv" joint="right_arm_m3_joint"      noise="0.0"/> 
        <jointvel name="ra4jv" joint="right_arm_m4_joint"      noise="0.0"/>
        <jointvel name="ra5jv" joint="right_arm_m5_joint"      noise="0.0"/>  
        <!-- <jointvel name="ra6jv" joint="right_arm_m6_joint"      noise="0.0"/>      -->



        <!--    扭矩电流传感器名称    关联的执行器名称     测量噪声标准差 -->
        <!-- 左腿 -->
        <actuatorfrc name="llpji"   actuator="llpj"  noise="0.0"/> <!-- Leg Left -->
        <actuatorfrc name="llrji"   actuator="llrj"  noise="0.0"/>
        <actuatorfrc name="llyji"   actuator="llyj"  noise="0.0"/>
        <actuatorfrc name="llkji"   actuator="llkj"  noise="0.0"/>
        <actuatorfrc name="llaji"   actuator="llaj"  noise="0.0"/>
        <!-- 右腿 -->
        <actuatorfrc name="rlpji"  actuator="rlpj"   noise="0.0"/> <!-- Leg Right -->
        <actuatorfrc name="rlrji"  actuator="rlrj"   noise="0.0"/>
        <actuatorfrc name="rlyji"  actuator="rlyj"   noise="0.0"/>
        <actuatorfrc name="rlkji"  actuator="rlkj"   noise="0.0"/>
        <actuatorfrc name="rlaji"  actuator="rlaj"   noise="0.0"/>
        
        <!-- 腰部 -->
        <actuatorfrc name="bm1ji"  actuator="bm1j"  noise="0.0"/>

        <!-- 左手臂 -->
        <actuatorfrc name="la1ji"  actuator="la1j"  noise="0.0"/> 
        <actuatorfrc name="la2ji"  actuator="la2j"  noise="0.0"/> 
        <actuatorfrc name="la3ji"  actuator="la3j"  noise="0.0"/> 
        <actuatorfrc name="la4ji"  actuator="la4j"  noise="0.0"/> 
        <actuatorfrc name="la5ji"  actuator="la5j"  noise="0.0"/>  
        <!-- <actuatorfrc name="la6ji"  actuator="la6j"  noise="0.0"/>  -->
        <!-- 右手臂 -->
        <actuatorfrc name="ra1ji"  actuator="ra1j"  noise="0.0"/> 
        <actuatorfrc name="ra2ji"  actuator="ra2j"  noise="0.0"/> 
        <actuatorfrc name="ra3ji"  actuator="ra3j"  noise="0.0"/>   
        <actuatorfrc name="ra4ji"  actuator="ra4j"  noise="0.0"/> 
        <actuatorfrc name="ra5ji"  actuator="ra5j"  noise="0.0"/>
        <!-- <actuatorfrc name="ra6ji"  actuator="ra6j"  noise="0.0"/> -->



        <!-- IMU陀螺仪（匹配真实硬件参数） 安装在imu站点   噪声       截止频率（Hz），用于低通滤波 -->
        <gyro          name="gyro"    site='imu' noise='0.005' cutoff='34.9'/>
        <!-- IMU加速度计                          噪声           截止频率（Hz） -->
        <accelerometer name="acc"     site='imu' noise='0.005' cutoff='157'/>
        <!-- 帧四元数传感器              测量站点对象     四元数噪声     关联的站点名称 -->
        <framequat     name='bq'      objtype='site' noise='0.001' objname='imu'/>
    </sensor>






</mujoco>
